                    if($scope.samples[i].Shipping_Type__c == 'Domestic'){
                        $scope.DomesticSamples.push($scope.samples[i]);
                    }else if($scope.samples[i].Shipping_Type__c == 'Export'){
                        $scope.ExportSamples.push($scope.samples[i]);
                    }
